@model List<ASPNETMVCRUD.Models.Domain.Employee>
@{
}
<h1>Employees</h1>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name </th>
            <th>Email</th>
            <th>DateTime</th>
            <th>Department</th>
            <td><a href="Employee"</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Id</td>
                <td>@employee.Name</td>
                <td>@employee.Email</td>
                <td>@employee.DateTime.ToString("dd-MM-yyyy")</td>
                <td>@employee.Department</td>
                <td><a href="Employees/View/@employee.Id">view</a></td>
            </tr>
        }

    </tbody>
</table>
